{
	"typeAcquisition": {
		"include": [ "jest" ]
	},
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "es2015",
		"noImplicitAny": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": true
	},
	"include": [
		"src/*"
	],
	"exclude": [
		"node_modules",
		"src/test/*"
	]
}